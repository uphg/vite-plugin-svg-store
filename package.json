{
  "name": "vite-plugin-svg-store",
  "version": "0.1.3",
  "description": "SVG store plugin for Vite",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": ["vite", "vite-plugin", "svg", "sprite", "store", "svgo"],
  "homepage": "https://github.com/uphg/vite-plugin-svg-store#readme",
  "repository": "uphg/vite-plugin-svg-store",
  "bugs": "uphg/vite-plugin-svg-store/issues",
  "author": "Lv Heng <lvheng233@gmail.com>",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "jest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "example:dev": "cd example && vite"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@rollup/plugin-typescript": "11.1.2",
    "@types/jest": "29.5.3",
    "@types/minimist": "1.2.2",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.46.0",
    "execa": "7.2.0",
    "fs-extra": "11.1.1",
    "jest": "29.6.2",
    "rollup": "3.27.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-typescript2": "0.35.0",
    "ts-node": "10.9.1",
    "tslib": "2.6.1",
    "typescript": "5.0.2",
    "vite": "^4.5.0",
    "vitepress": "1.0.0-beta.7"
  },
  "peerDependencies": {
    "vite": "^4.5.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "svg-baker": "^1.7.0",
    "svgo": "^3.0.3"
  }
}